{{#if succ}}
    <div role="notification" class="alert alert-success">
        {{succ}}
    </div>
{{/if}}

{{#if err}}
    <div role="notification" class="alert alert-danger">
        {{err}}
    </div>
{{/if}}

{{#if error}}
    <div role="notification" class="alert alert-danger">
        {{error}}
    </div>
{{/if}}
